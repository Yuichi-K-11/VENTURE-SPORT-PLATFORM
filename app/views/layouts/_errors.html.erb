<% if obj.errors.any? %>
  <div id="error_explanation" class="alert alert-danger col-12">
    <h3><%= obj.errors.count %>件のエラー</h3>
    <ul>
      <% obj.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  </div>
<% end %><%= render 'layouts/errors', model: f.object %>